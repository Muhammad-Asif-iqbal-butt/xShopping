@using MVC.Models
@model List<Sub_Categories>
@{
    Layout = null;
}

<div class="form-group">
    <label class="col-lg-3 control-label" style="font-size:20px">Sub Categories: </label>
    <div class="col-lg-3 ">
        <select name="sub_category" style="font-size:20px" >

            @foreach (var v in ViewBag.cat)
            {
                <option >@v.Name</option>
            }
        </select>

        <div id="d1">
        </div>
    </div>
</div>













